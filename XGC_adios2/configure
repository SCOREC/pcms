          MPILIBS="-lmpich -lmtl_common -lvapi -lmosal -lmpga -lpthread"
          MPIF77LIBS="-lmpichfarg"
          MPIFCLIBS="-lmpichf90nc $MPIF77LIBS"
        fi
        MPICXXLIBS="-lpmpich++"
        MPICXXINCLUDES="-I$MPI_INCDIR/mpi2c++"
        MPICXXDEFINES="-DHAVE_MPI_CPP"
        MPIDEFINES="-DHAVE_MPI -DUSE_STDARG -DHAVE_STDLIB_H=1 -DHAVE_STRING_H=1 -DHAVE_UNISTD_H=1 -DHAVE_STDARG_H=1 -DUSE_STDARG=1 -DMALLOC_RET_VOID=1"
        ;;
        nid0*) # franklin: Cray XT4 at NERSC
