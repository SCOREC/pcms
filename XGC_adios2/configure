        MPIINCLUDES="-I/usr/lpp/ppe.poe/include/thread64 -I/opt/rsct/lapi/include64 -I/usr/lpp/ssp/css/aix52/include64 -I/opt/rsct/lapi/include"
        # Use reentrant version of IBM compilers on Bassi
        if test ".$CXX" = .xlC; then CXX="xlC_r"; fi
        if test ".$CC" = .xlc; then CC="xlc_r"; fi
        if test ".$F77" = .xlf; then F77="xlf_r"; fi
        if test ".$FC" = .xlf90; then FC="xlf90_r"; fi
