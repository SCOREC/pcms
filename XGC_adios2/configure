  case "$build" in
    *aix*)
    ARFLAGS="-crusX64"
    LDFLAGS="-b64 $LDFLAGS"
    case "$CXX" in
      *xlC*)    CXXFLAGS="-q64 $CXXFLAGS"
		CXX_AR="$AR $ARFLAGS"  ;;
      *g++*)    CXXFLAGS="-maix64 $CXXFLAGS"
		CXX_AR="$AR $ARFLAGS"  ;;
    esac
    case "$CC" in
      *xlc*)    CFLAGS="-q64 $CFLAGS"  ;;
      *gcc*)    CFLAGS="-maix64 $CFLAGS"  ;;
    esac
    case "$F77" in
      *xlf*)      FFLAGS="-q64 $FFLAGS"  ;;
      *gfortran*) FFLAGS="-maix64 $FFLAGS"  ;;
    esac
    case "$FC" in
      *xlf90*)    FCFLAGS="-q64 $FCFLAGS"  ;;
      *gfortran*) FCFLAGS="-maix64 $FCFLAGS"  ;;
    esac
    if test "$ac_cv_cpes_defaults" = yes; then
      case $ax_cpes_defaults_hostname in
        seaborg*) # Point to 64-bit NetCDF/HDF5 installation
        NETCDF_DIR=
	HDF5_DIR=
	# Large pages loader flag
        # LDFLAGS="-blpdata $LDFLAGS"
        # Use reentrant version of IBM compilers on Seaborg
        if test ".$CXX" = .xlC; then CXX="xlC_r"; fi
        if test ".$CC" = .xlc; then CC="xlc_r"; fi
        if test ".$F77" = .xlf; then F77="xlf_r"; fi
        if test ".$FC" = .xlf90; then FC="xlf90_r"; fi
	;;
      esac
    fi
    ;;
  esac
else
  case "$build" in
    *aix*)
    ARFLAGS="-crusX32"
    LDFLAGS="-b32 -bmaxdata:0x70000000 -bmaxstack:0x10000000 $LDFLAGS"
    case "$CXX" in
      *xlC*)    CXXFLAGS="-q32 $CXXFLAGS"
		CXX_AR="$AR $ARFLAGS"  ;;
    esac
    case "$CC" in
      *xlc*)    CFLAGS="-q32 $CFLAGS"  ;;
    esac
    case "$F77" in
      *xlf*)    FFLAGS="-q32 $FFLAGS"  ;;
    esac
    case "$FC" in
      *xlf90*)  FCFLAGS="-q32 $FCFLAGS"  ;;
    esac
    ;;
  esac
fi


