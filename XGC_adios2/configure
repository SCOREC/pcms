        CAMTIMERS_FLAGS="-DCAM_TIMERS $ax_cv_f90_modflag$srcdir/../camtimers"
        CAMTIMERS_FCFLAGS="-Mfree -i4 -Mdalign -byteswapio -DLINUX -DFORTRANUNDERSCORE $MPIDEFINES -DHAVE_NANOTIME -DBIT64 -I$srcdir/../camtimers $MPIINCLUDES"
        CAMTIMERS_CFLAGS="-DLINUX -DFORTRANUNDERSCORE $MPIDEFINES -DHAVE_NANOTIME -DBIT64 -I$srcdir/../camtimers $MPIINCLUDES"
        ;;
        *ifort*|*ifc*) # Intel Fortran compiler
        CAMTIMERS_FLAGS="-DCAM_TIMERS $ax_cv_f90_modflag$srcdir/../camtimers"
        CAMTIMERS_FCFLAGS="-free -i4 -align -convert little_endian -DLINUX -DFORTRANUNDERSCORE $MPIDEFINES -DHAVE_NANOTIME -DBIT64 -I$srcdir/../camtimers $MPIINCLUDES"
        CAMTIMERS_CFLAGS="-DLINUX -DFORTRANUNDERSCORE $MPIDEFINES -DHAVE_NANOTIME -DBIT64 -I$srcdir/../camtimers $MPIINCLUDES"
        ;;
        *ftn*) # Cray Fortran cross-compiler
	case "$ac_cv_cpes_compiler_vendor" in
	  pgi)     # E.g., Hopper
          CAMTIMERS_FLAGS="-DCAM_TIMERS $ax_cv_f90_modflag$srcdir/../camtimers"
          CAMTIMERS_FCFLAGS="-Mfree -i4 -Mdalign -byteswapio -DLINUX -DFORTRANUNDERSCORE $MPIDEFINES -DHAVE_NANOTIME -DBIT64 -I$srcdir/../camtimers $MPIINCLUDES"
          CAMTIMERS_CFLAGS="-DLINUX -DFORTRANUNDERSCORE $MPIDEFINES -DHAVE_NANOTIME -DBIT64 -I$srcdir/../camtimers $MPIINCLUDES"
          ;;
	  intel)    # E.g., Edison
          CAMTIMERS_FLAGS="-DCAM_TIMERS $ax_cv_f90_modflag$srcdir/../camtimers"
          CAMTIMERS_FCFLAGS="-free -i4 -align -convert little_endian -DLINUX -DFORTRANUNDERSCORE $MPIDEFINES -DHAVE_NANOTIME -DBIT64 -I$srcdir/../camtimers $MPIINCLUDES"
          CAMTIMERS_CFLAGS="-DLINUX -DFORTRANUNDERSCORE $MPIDEFINES -DHAVE_NANOTIME -DBIT64 -I$srcdir/../camtimers $MPIINCLUDES"
          ;;
        esac
        ;;
        *xlf*) # IBM Fortran compiler
        CAMTIMERS_FLAGS="-WF,-DCAM_TIMERS $ax_cv_f90_modflag$srcdir/../camtimers"
        CAMTIMERS_FCFLAGS="-WF,-DAIX -WF,-DFORTRAN_SAME -WF,$MPIDEFINES -WF,-DBGP -WF,-DTHREADED_OMP -I$srcdir/../camtimers $MPIINCLUDES"
        CAMTIMERS_CFLAGS="-DAIX -DFORTRAN_SAME $MPIDEFINES -DBGP -DTHREADED_OMP -I$srcdir/../camtimers $MPIINCLUDES"
        ;;
      esac
#     Build specific CAM timing library locally
      if test "$ac_cv_cpes_with_papi" = no; then
        CAMTIMERS_LIBS="-L. -ltimers"
        CAMTIMERS_DEPS="./libtimers.a"
      else
        CAMTIMERS_LIBS="-L. -ltimers_wpapi"
        CAMTIMERS_DEPS="./libtimers_wpapi.a"
      fi
    fi
else

# default is enable if camtimers source is present
    if test -d "$srcdir/../camtimers"; then
      with_camtimers="yes"
      case "$FC" in
        *pgf*) # PGI Fortran compiler
        CAMTIMERS_FLAGS="-DCAM_TIMERS $ax_cv_f90_modflag$srcdir/../camtimers"
        CAMTIMERS_FCFLAGS="-Mfree -i4 -Mdalign -byteswapio -DLINUX -DFORTRANUNDERSCORE $MPIDEFINES -DHAVE_NANOTIME -DBIT64 -I$srcdir/../camtimers"
        CAMTIMERS_CFLAGS="-DLINUX -DFORTRANUNDERSCORE $MPIDEFINES -DHAVE_NANOTIME -DBIT64 -I$srcdir/../camtimers"
        ;;
        *ifort*|*ifc*) # Intel Fortran compiler
        CAMTIMERS_FLAGS="-DCAM_TIMERS $ax_cv_f90_modflag$srcdir/../camtimers"
        CAMTIMERS_FCFLAGS="-free -i4 -align -convert little_endian -DLINUX -DFORTRANUNDERSCORE $MPIDEFINES -DHAVE_NANOTIME -DBIT64 -I$srcdir/../camtimers $MPIINCLUDES"
        CAMTIMERS_CFLAGS="-DLINUX -DFORTRANUNDERSCORE $MPIDEFINES -DHAVE_NANOTIME -DBIT64 -I$srcdir/../camtimers $MPIINCLUDES"
        ;;
        *ftn*) # Cray Fortran cross-compiler
	case "$ac_cv_cpes_compiler_vendor" in
	  pgi)     # E.g., Hopper
          CAMTIMERS_FLAGS="-DCAM_TIMERS $ax_cv_f90_modflag$srcdir/../camtimers"
          CAMTIMERS_FCFLAGS="-Mfree -i4 -Mdalign -byteswapio -DLINUX -DFORTRANUNDERSCORE $MPIDEFINES -DHAVE_NANOTIME -DBIT64 -I$srcdir/../camtimers $MPIINCLUDES"
          CAMTIMERS_CFLAGS="-DLINUX -DFORTRANUNDERSCORE $MPIDEFINES -DHAVE_NANOTIME -DBIT64 -I$srcdir/../camtimers $MPIINCLUDES"
          ;;
	  intel)    # E.g., Edison
          CAMTIMERS_FLAGS="-DCAM_TIMERS $ax_cv_f90_modflag$srcdir/../camtimers"
          CAMTIMERS_FCFLAGS="-free -i4 -align -convert little_endian -DLINUX -DFORTRANUNDERSCORE $MPIDEFINES -DHAVE_NANOTIME -DBIT64 -I$srcdir/../camtimers $MPIINCLUDES"
          CAMTIMERS_CFLAGS="-DLINUX -DFORTRANUNDERSCORE $MPIDEFINES -DHAVE_NANOTIME -DBIT64 -I$srcdir/../camtimers $MPIINCLUDES"
          ;;
        esac
        ;;
        *xlf*) # IBM Fortran compiler
        CAMTIMERS_FLAGS="-WF,-DCAM_TIMERS $ax_cv_f90_modflag$srcdir/../camtimers"
        CAMTIMERS_FCFLAGS="-WF,-DAIX -WF,-DFORTRAN_SAME -WF,$MPIDEFINES -WF,-DBGP -WF,-DTHREADED_OMP -I$srcdir/../camtimers"
        CAMTIMERS_CFLAGS="-DAIX -DFORTRAN_SAME $MPIDEFINES -DBGP -DTHREADED_OMP -I$srcdir/../camtimers"
        ;;
      esac
#     Build specific CAM timing library locally
      if test "$ac_cv_cpes_with_papi" = no; then
        CAMTIMERS_LIBS="-L. -ltimers"
        CAMTIMERS_DEPS="./libtimers.a"
      else
        CAMTIMERS_LIBS="-L. -ltimers_wpapi"
        CAMTIMERS_DEPS="./libtimers_wpapi.a"
      fi
    else
      with_camtimers="no"
    fi
fi

  ac_cv_cpes_with_camtimers="$with_camtimers"
fi
{ $as_echo "$as_me:$LINENO: result: $ac_cv_cpes_with_camtimers" >&5
$as_echo "$ac_cv_cpes_with_camtimers" >&6; }





