		FC_DEBUG_FLAGS="-g -fbounds-check -Wall"
		if test $gcc_release -gt "0" ; then
			FCFLAGS="$FCFLAGS -fcray-pointer"
			FC_DEBUG_FLAGS="$FC_DEBUG_FLAGS -ffpe-trap=invalid,zero,overflow"
		fi
		if test $gcc_release -gt "1" ; then
			FC_OMP_FLAGS="-fopenmp"
		fi
	;;
	*ifort*|*ifc*) # Intel Fortran compiler   http://www.intel.com
		FC_OPT_FLAGS="-O3 -ip -pad -funroll-loops -fno-alias -safe_cray_ptr"
		FC_DEBUG_FLAGS="-g -check all"
		FC_OMP_FLAGS="-openmp"
	;;
	*xlf90*) # IBM Fortran 90   http://www.ibm.com
		FCFLAGS="$FCFLAGS -qsuffix=cpp=F90"
		FC_OPT_FLAGS="-O3 -qstrict -qarch=auto -qtune=auto -qessl"
		FC_DEBUG_FLAGS="-g -qcheck -qflttrap=enable:zerodivide:invalid:overflow -qsigtrap"
		FC_OMP_FLAGS="-qsmp=omp"
	;;
	*pgf9*) # Portland Group F90/F95 compiler   http://www.pgroup.com
		FC_OPT_FLAGS="-fast -Kieee"
		FC_DEBUG_FLAGS="-g -Mbounds -Mchkptr"
		FC_OMP_FLAGS="-mp"
	;;
	*pathf9*) # Pathscale pathf95 compiler   http://www.pathscale.com
		$FC -v >& pathsc_tmp.out
		PATHSC_V=`grep Version pathsc_tmp.out`
                rm -f pathsc_tmp.out
		pathsc_version=`expr "$PATHSC_V" : '.* \([0-9]\)\..*'`
		if test $pathsc_version -lt "3" ; then
		        # -LNO:hmb=off workaround for compiler optimizer bug
		        # that will be fixed in PathScale 3.0 compiler
		        FC_OPT_FLAGS="-Ofast -LNO:hmb=off"
                else
		        FC_OPT_FLAGS="-Ofast"
                fi
		FC_DEBUG_FLAGS="-g -C"
		FC_OMP_FLAGS="-mp"
	;;
	*ftn*) # Cray Fortran cross-compiler
	        case "$ac_cv_cpes_compiler_vendor" in
		pgi)     # E.g., Hopper
		   FC_OPT_FLAGS="-fastsse -Kieee"
		   FC_DEBUG_FLAGS="-g"
		   FC_OMP_FLAGS="-mp"
                ;;
		intel)    # E.g., Edison
		   FC_OPT_FLAGS="-O3 -ip -pad -funroll-loops -fno-alias -safe_cray_ptr"
		   FC_DEBUG_FLAGS="-g -check all"
		   FC_OMP_FLAGS="-openmp"
	     	;;
		*)
		   { { $as_echo "$as_me:$LINENO: error: Unknown compiler $FC on target $build" >&5
$as_echo "$as_me: error: Unknown compiler $FC on target $build" >&2;}
   { (exit 1); exit 1; }; }
	        ;;
		esac
	;;
	*)
		ac_fc_flags_preset=no
	;;
	esac
	{ $as_echo "$as_me:$LINENO: result: yes" >&5
$as_echo "yes" >&6; }
else
	{ $as_echo "$as_me:$LINENO: result: no" >&5
$as_echo "no" >&6; }
