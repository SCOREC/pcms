        MPIFCLIBS="-lmpi_f90 $MPIF77LIBS"
        MPICXXLIBS="-lmpi_cxx"
        MPICXXINCLUDES="-I$MPI_INCDIR/openmpi"
        MPICXXDEFINES="-DOMPI_WANT_CXX_BINDINGS=1"
        MPIDEFINES="-DHAVE_MPI"
